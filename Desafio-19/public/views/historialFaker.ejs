<h2 style="color:crimson;">Historial</h2>

<% if(productos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Titulo</th> <th>Precio</th> <th>Imagen</th> </tr>
            <% productos.forEach(producto=> { %>
            <tr>
                <td><%=producto.titulo%></td>
                <td><%=producto.precio%></td>
                <td><img style="width: 2rem; height: 'auto'" src="<%= producto.imagen %>"></td>
            </tr>
            <% }) %>
        </table>
    </div>
<% } else{ %>
    <h3 class="alert alert-danger">no se encontraron datos</h3>
<% } %>